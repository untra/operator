{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "workspaces": {
    ".": {
      "entry": ["src/standalone.ts"]
    },
    "packages/app": {
      "entry": ["src/index.tsx"]
    },
    "packages/backend": {
      "entry": ["src/index.ts"]
    },
    "packages/plugins/plugin-issuetypes": {}
  },
  "ignore": [
    "**/*.d.ts",
    "**/dist/**",
    "**/__tests__/test-utils/**"
  ],
  "ignoreDependencies": [
    "@backstage/*",
    "raw-loader"
  ],
  "ignoreExportsUsedInFile": true
}
